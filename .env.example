# Local environment overrides
# Copy to .env.local and update values for your machine

# Root directory containing participant folders (e.g., #1011)
AE_DATA_ROOT=/path/to/studies

# PostgreSQL connection strings
# Main database for processing actual data
AE_DATABASE_URL=postgresql+psycopg://user:password@localhost:5432/acoustic_emissions

# Test database (optional - used for running tests)
# If not set, tests will default to acoustic_emissions_test
# AE_TEST_DATABASE_URL=postgresql+psycopg://user:password@localhost:5432/acoustic_emissions_test
